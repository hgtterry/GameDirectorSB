material nxogre.forcefield
{
  technique
  {
    pass
    {
      scene_blend alpha_blend
      lighting off
      depth_write off

      texture_unit
      {
        scroll_anim 0.01 0
        texture nxogre.forcefield.png
      }
    }
  }
}


material ground-from-nxogre.org
{
  technique
  {
    pass
    {
      emissive 1 1 1 1
      scene_blend alpha_blend
      texture_unit
      {
        texture nxogre.floor.png
        scale 0.002 0.002
        colour_op modulate
      }
      
      texture_unit
      {
        texture nxogre.floor.shadowalpha.png
        colour_op_ex modulate_x2 src_current src_texture
      }
    }
  }
}



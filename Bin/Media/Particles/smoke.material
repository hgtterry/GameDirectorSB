
material Examples/Smoke
{
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_write off

			texture_unit
			{
				texture smoke.png
				tex_address_mode clamp
			}
		}
	}
}

material Examples/Smoke_1
{
	technique
	{
		pass
		{
			lighting off
			scene_blend alpha_blend
			depth_write off
			diffuse 1 1 1 0.25

			texture_unit
			{
				texture smoke_new.png
				tex_address_mode clamp
			}
		}
	}
}
